{
  "name": "@withoutbg/ui",
  "version": "0.1.0",
  "description": "WithoutBG UI Component Library - Reusable React components with Tailwind CSS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./tailwind-preset": "./tailwind.preset.js",
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist",
    "tailwind.preset.js",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^10.0.1",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^10.0.1",
    "@storybook/react-vite": "^10.0.1",
    "@storybook/test": "^8.4.7",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "storybook": "^10.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/withoutbg/ui.git"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "tailwindcss",
    "withoutbg"
